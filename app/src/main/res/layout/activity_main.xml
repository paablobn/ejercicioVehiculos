<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/txtBiciMain"
        android:layout_width="349dp"
        android:layout_height="38dp"
        android:layout_marginTop="72dp"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.483"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnBicicletaMain"
        android:layout_width="347dp"
        android:layout_height="40dp"
        android:layout_marginTop="20dp"
        android:text="ADD BICICLETA"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtBiciMain"
        app:strokeColor="#000000" />

    <TextView
        android:id="@+id/txtCocheMain"
        android:layout_width="342dp"
        android:layout_height="33dp"
        android:layout_marginTop="67dp"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnBicicletaMain" />

    <Button
        android:id="@+id/btnCocheMain"
        android:layout_width="341dp"
        android:layout_height="44dp"
        android:layout_marginTop="20dp"
        android:text="add coche"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtCocheMain" />

    <TextView
        android:id="@+id/txtMotoMain"
        android:layout_width="335dp"
        android:layout_height="32dp"
        android:layout_marginTop="106dp"
        android:textAlignment="center"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnCocheMain" />

    <Button
        android:id="@+id/btnMotoMain"
        android:layout_width="332dp"
        android:layout_height="40dp"
        android:layout_marginTop="18dp"
        android:text="add moto"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtMotoMain" />
</androidx.constraintlayout.widget.ConstraintLayout>